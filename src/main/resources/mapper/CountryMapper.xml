<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.plus.dao.CountryMapper">
    <select id="country" resultType="com.example.plus.entity.Country">
        select * from country
    </select>
    <select id="getCountry" resultType="com.example.plus.entity.Country">
        select * from country where Code = #{code}
    </select>
</mapper>